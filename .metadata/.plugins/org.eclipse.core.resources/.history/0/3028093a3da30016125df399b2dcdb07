package Beans;

import java.util.*;

public abstract class Outils {
	
	public static void decouperListDocumentSujet(Document d,Sujet s){
			if(d.getNom()=="document à importer"){
				System.out.println("document à importer");
				s.addListeDocumentsAImporter(d);
			}
			else if(d.getNom()=="correction"){
				s.setCorrection(d);
			}
			else if(d.getNom()=="sujet"){
				s.setSujet(d);
			}
		
	}

}
