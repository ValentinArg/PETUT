package Beans;

import java.util.*;

public class Forum {

	private int id;
	private String Type; // type = vie etudiante ou vie scolaire
	private String nom;
	private List<Sujet> listeSujets;
	
	public Forum(int id, String type, String nom) {
		super();
		this.id = id;
		Type = type;
		this.nom = nom;
	}
	
	
	
	
}
