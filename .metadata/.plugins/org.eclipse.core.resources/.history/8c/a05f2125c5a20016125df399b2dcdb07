package Beans;

public class Document {
	
	private int idDocument;
	private int idSujet;
	private int idUtilisateur;
	private String nom;
	private String lien;
	private String date;
	public Document(int idDocument, String nom, String lien, String date) {
		super();
		this.idDocument = idDocument;
		this.nom = nom;
		this.lien = lien;
		this.date = date;
	}
	
	
	
	
	
}
