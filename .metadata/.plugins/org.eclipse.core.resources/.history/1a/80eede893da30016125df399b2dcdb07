package Beans;

import java.util.*;

public abstract class Outils {
	
	public static void decouperListDocumentSujet(Document d,String nom,Sujet s){
		System.out.println("je suis la");
		if(nom=="document à importer"){
			System.out.println("document à importer");
			s.addListeDocumentsAImporter(d);
		}
		else if(d.getNom()=="correction"){
			System.out.println("correction");
			s.setCorrection(d);
		}
		else if(d.getNom()=="sujet"){
			System.out.println("sujet");
			s.setSujet(d);
		}
		
	}

}
